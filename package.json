{
  "name": "podium-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/serve.js",
  "scripts": {
    "start": "npm run sync && nodemon --exec babel-node src/serve.js",
    "sync": "aws s3 sync ./data s3://podium-core --exclude '*DS_Store'",
    "build": "npm run sync && babel src -d dist",
    "serve": "node dist/serve.js",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "pm2 deploy ecosystem.config.js production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@carter_andrewj/podix": "^2.6.21",
    "aws-sdk": "^2.384.0",
    "body-parser": "^1.18.3",
    "busboy-body-parser": "^0.3.2",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "lokijs": "^1.5.6",
    "radixdlt": "^1.0.6-alpha"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "nodemon": "^1.18.9",
    "pm2": "^3.2.4"
  }
}
